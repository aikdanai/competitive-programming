{
  "files.associations": {
    "*.prisma": "graphql",
    "ostream": "cpp",
    "iostream": "cpp",
    "vector": "cpp",
    "functional": "cpp",
    "*.tcc": "cpp"
  },
  "code-runner.executorMap": {
    // "cpp": "cd $dir && g++ -std=c++17 $fileName -o sol && $dirWithoutTrailingSlash/sol"
    "cpp": "cd $dir && g++ -std=c++17 $fileName -o sol && cat $dirWithoutTrailingSlash/in.txt | $dirWithoutTrailingSlash/sol > $dirWithoutTrailingSlash/out.txt"
  }
}